        			var url = window.location.href;
        			var rtn = getCurrentUrl(url).getPath() + '?' + getCurrentUrl(url).getQuery();
        			location.href = "/ko/members/login.do?url="+ encodeURIComponent(rtn);
        		}
        	} else if (data.result == 'NG0') {
        		alert('잘못된 접근입니다.');    			
        	} else if (data.result == 'NG1') {
				if(device == 'Y'){
					alert('App 사용자만 참여 가능합니다.');
				} else {
					alert('모바일웹으로만 예약  가능합니다.');						
				}          		
        		//alert("쿠폰번호가 올바르지 않습니다.");    			
        	} else if (data.result == 'NG2') {
        		alert('프로모션 기간이 아닙니다.');
        		//alert("쿠폰번호가 올바르지 않습니다.");
        	} else if (data.result == 'NG3') {
        		alert('해당 프로모션 참여 대상자가 아니십니다.');
        		//alert("쿠폰번호가 올바르지 않습니다.");        		
        	} else {
        		alert('해당 프로모션 참여 대상자가 아니십니다.');        		
        		//alert("쿠폰번호가 올바르지 않습니다.");
        	}        		
        }
    })	
}

