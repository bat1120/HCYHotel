<script type="text/javascript">
function fnSrchFromLayer(isB, type, hcode, prodid, showfg) {
	var prod_type = '';
	var srch_type = 'RM';
	var show_fg_val = '';
	var adlt = '2';
	var chld = '0';
	var psnif = '1:2:0';
	var eventid = '';
	var showfgtemp = showfg;

	var showcp = '';
	if (showfgtemp.indexOf("|") > 0) {
		showfg = showfgtemp.substr(0 , showfgtemp.indexOf("|"));
		showcp = showfgtemp.substr(showfgtemp.indexOf("|") + 1);
	}	
	
	if (type == 'R' || type == 'ER') {
		prod_type = 'M0801';
		srch_type = 'RM';
	} else if (type == 'P' || type == 'EP') {
		prod_type = 'M0802';
		srch_type = 'RM';
		show_fg_val = showfg;
	} else if (type == 'D' || type == 'ED' ) {
		prod_type = 'M0803';
		srch_type = 'DN';
		show_fg_val = showfg;
		adlt = '2';
		chld = '0';
		psnif = '2:0';
	}
	
	if (isB == 'B') {
		var goUrl = ambContext + "/novotel/gangnam/" + ambLangCd + "/reservationStep1.do";
	} else {
		var goUrl = ambContext + "/" + ambLangCd + "/reservation/reservationStep1.do";
	}
	
	if (type == 'ER' || type == 'EP' || type == 'ED') {
		eventid = prodid;
	}
	
	if (prodid == 'PRDPK02746' || prodid == 'PRDPK02777' || prodid == 'PRDPK02778' || prodid == 'PRDPK02814' || prodid == 'PRDPK02815') {
		//$("#rsv_rdotype").val("FR");
	}

	if (prodid == 'PRDPK02788' || prodid == 'PRDPK02790' || prodid == 'PRDPK02791' || prodid == 'PRDPK02792' || prodid == 'PRDPK02784'  || prodid == 'PRDPK02793' || prodid == 'PRDPK02794') {
		//$("#rsv_rdotype").val("FR");
	}
	
	if (showcp.trim() != '' ) {
		$("#rsv_rdotype").val("FR");
		$("#rsv_rdotype_code").val(showcp);
	}
	
	
	$("#lyr_rsv_type").val(srch_type);
	$("#lyr_rsv_hcode").val(hcode);
	$("#lyr_rsv_prdno").val($.trim(prodid));
	$("#lyr_rsv_prdkind").val(prod_type);
	$("#lyr_rsv_show_fg").val(show_fg_val);
	$("#lyr_rsv_eventid").val($.trim(eventid));
	
	$("#lyr_rsv_adult").val(adlt);
	$("#lyr_rsv_child").val(chld);
	$("#lyr_rsv_maxadult").val(adlt);
	$("#lyr_rsv_maxchild").val(chld);
	$("#lyr_rsv_personinfo").val(psnif);
	
	$("#srchFromLayer").attr("action", goUrl);
	$("#srchFromLayer").submit();
}
function fnShowReservationLayer(type, hcode, prodid, showfg) {
	
		fnSrchFromLayer('B', type, hcode, prodid, showfg);
		
	
}

function fnShowDiningReservationLayer(type, hcode, prodid, showfg) {
	if (confirm("회원전용 프로모션 입니다.\n로그인 페이지로 이동 하시겠습니까?")) {
		var url = window.location.href;
      	var rtn = getCurrentUrl(url).getPath() + '?' + getCurrentUrl(url).getQuery();
      	location.href = "/ko/members/login.do?url="+ encodeURIComponent(rtn);
    }
}

function fnShowPromotionReservationLayer(type, hcode, prodid, showfg, device) {
	$.ajax({
        url:'/ko/offer/checkEventPromotionJson.do',
        data:"prod_id="+prodid,
        dataType:'json',
        success:function(data) {
        	if (data.result == 'OK') {
	       		
    	    		fnSrchFromLayer('B', type, hcode, prodid, showfg);
        			
        		
        	} else if (data.result == 'LOGIN') {
        		//alert('회원전용 프로모션 입니다. 로그인 화면으로 이동합니다.');
